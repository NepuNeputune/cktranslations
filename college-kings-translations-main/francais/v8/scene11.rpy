# game/v8/scene11.rpy:107
translate francais wolves_gym_conf_b1f0ff7d:

    # u "One... Two... Three..."
    u "Un... deux... trois..."

# game/v8/scene11.rpy:115
translate francais wolves_gym_cool_b00edad9:

    # u "Seems to be working out great. Can you spot me?"
