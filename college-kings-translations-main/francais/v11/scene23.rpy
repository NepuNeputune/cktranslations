    # ch "*Sigh* Give me a second Sebastian."
    ch "*Soupir* Donne-moi une seconde Sebastian."

# game/v11/scene23.rpy:64
translate francais v11s23_chris1_7c77aca1:

    # ch "What's up?"
    ch "Qu'est-ce qu'il y a ?"

# game/v11/scene23.rpy:71
